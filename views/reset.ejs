<% include ./partials/header %>





<div class="container logincontainer">
        <div class="row">
                <div class=" loginbox col-md-5 col-sm-12 col-xs-12">
                        <div class="loginboxheading">RESET PASSWORD</div>

                        <form class="ui form form col-md-12 col-xs-12 loginform" action="/reset/:token" method="POST"> 

                        <div class="field">
                                <input type="password" name="password" placeholder="password" required>
                        </div>         
                        <div class="field">
                            <input type="password" name="confirm" placeholder="password" required>
                        </div>
                        <button type="submit" class="btn btn-default btn-lg loginsubmitbutton">UPDATE PASSWORD </button>

                        </form>
                       
        </div>

        <div class="col-md-7 loginpageimage">
                <img alt="Image" height="420" width="600" src="/images/loginpageimage.jpg">
        </div>

     </div>         
</div>
